{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "StratusLogger",
  "icons": {
    "16": "/icon16.png",
    "32": "/icon32.png",
    "192": "/icon192.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-xr+HqUtnq8KF3IBn+TQ37Vz0elxg11bPYmNJy5M/p38='; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://cbschi-strat1.cbs.ad.cbs.net/webclient",
        "*://cbschi-strat1/webclient/"
      ],
      "js": [
        "/static/js/2.chunk.js",
        "/static/js/main.chunk.js",
        "/static/js/runtime-main.js"
      ],
      "css": ["/static/css/main.chunk.css", "/static/css/2.chunk.css"]
    }
  ],
  "permissions": ["storage"],
  "web_accessible_resources": ["/static/*"]
}
